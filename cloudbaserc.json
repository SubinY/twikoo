{
  "version": "2.0",
  "envId": "{{envId}}",
  "functionRoot": "./src/function",
  "functions": [
    {
      "name": "twikoo",
      "timeout": 30,
      "runtime": "Nodejs10.15",
      "memorySize": 128,
      "handler": "index.main"
    }
  ],
  "framework": {
    "name": "twikoo",
    "plugins": {
      "manage": {
        "use": "@cloudbase/framework-plugin-website@1.6.0-alpha.0",
        "inputs": {
          "installCommand": "echo 'Skip Install'",
          "buildCommand": "echo 'npm run build'",
          "outputPath": "/dist",
          "cloudPath": "{{env.deployManagePath}}",
          "ignore": [
            ".git",
            ".github",
            "node_modules",
            "cloudbaserc.js"
          ]
        }
      },
      "requirement": {
        "environment": {
          "deployManagePath": {
            "description": "部署路径",
            "required": true,
            "default": "/dist"
          }
        }
      },
      "function": {
        "use": "@cloudbase/framework-plugin-function",
        "inputs": {
          "functionRootPath": "./src/function",
          "functions": [
            {
              "name": "twikoo",
              "timeout": 30,
              "envVariables": {},
              "runtime": "Nodejs10.15",
              "memory": 128
            }
          ],
          "servicePaths": {
            "twikoo": "/twikoo"
          }
        }
      }
    }
  }
}
